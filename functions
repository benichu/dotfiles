function ss  {
  if [ -e script/rails ]; then # Rails 3
    script/rails server $@
  elif [ -e script/server ]; then # Rails 2
    script/server $@
  else # Rails 4
    rails server $@
  fi
}
function sc {
  if [ -e script/rails ]; then # Rails 3
    script/rails console $@
  elif [ -e script/console ]; then # Rails 2
    script/console $@
  else # Rails 4
    rails console $@
  fi
}
function sg {
  if [ -e script/rails ]; then # Rails 3
    script/rails generate $@
  if [ -e script/generate ]; then # Rails 2
    script/generate $@
  else # Rails 4
    rails generate $@
  fi
}
function isreg {
  whois $1 | grep -q 'No match' && echo "No" || echo "Yes"
}
# Create a new directory and enter it
function mkd() {
  mkdir -p "$@" && cd "$@"
}
git_prompt() {
  local g="$(__gitdir)"
  if [ -n "$g" ]; then
  # The __git_ps1 function inserts the current git branch where %s is
      local GIT_PROMPT=`__git_ps1 " â†’ %s"`
      echo ${GIT_PROMPT}
  fi
}
